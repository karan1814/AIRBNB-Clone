<%  if(successmsg && successmsg.length){ %>
    <div class="alert alert-success alert-dismissible fade show col-5 offset-3" role="alert" >
        <%= successmsg %>
        <button type="button" class="btn-close offset-8" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<% } %>

<%  if(errormsg && errormsg.length){ %>
    <div class="alert alert-danger alert-dismissible fade show col-5 offset-3" role="alert" >
        <%= errormsg %>
        <button type="button" class="btn-close offset-8" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<% } %>